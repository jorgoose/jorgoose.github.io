              <li>
                <div class="card">
                  <div class="card__image">
                    <img
                      src="https://content.cdntwrk.com/files/aHViPTExOTYyMCZjbWQ9aXRlbWVkaXRvcmltYWdlJmZpbGVuYW1lPWl0ZW1lZGl0b3JpbWFnZV81ZmMxNDkxNmU3YjA1LnBuZyZ2ZXJzaW9uPTAwMDAmc2lnPTBmZTM4MGIxMGVjNTEwNzNmOGU5ZTE4YTVmNWQyNDI2"
                      alt="Other Project 2"
                    />
                  </div>
                  <div class="card__content">
                    <h3 class="card__title">
                      Capital One SWE Summit Hackathon
                    </h3>
                    <div class="card__tags">
                      <span class="card__tag">2nd Place</span>
                      <p class="card__description">
                        This is a description of the project.
                      </p>
                    </div>
                  </div>
                </div>
              </li>
            </li>
          </Grid>
          <ContactCTA />
        </div>
      </div>

      <style>
        /* Logo styling for each company in a card */
        .logo {
          width: 25%;
          height: 100%;
          object-fit: contain;
          border-radius: 0.25rem;
        }

        /* Keep same logo height but stretch the image slightly horizontally. It should occupy the same space overall, but the image should be stretched. */
        .logo--stretch {
          width: 20rem;
          height: 5rem;
          object-fit: stretch;
        }

        .line {
          width: 100%;
          height: 1px;
          background-color: var(--color-gray-300);
        }

        /* Style for card__tag that makes it like a post tag or button type shape */
        .card__tag {
          display: inline-block;
          border-radius: 0.25rem;
          background-color: var(--color-primary);
          color: var(--color-white);
          font-size: 0.75rem;
          font-weight: 600;
          text-transform: uppercase;
        }

        /* Style for card, card__image, and card__content */
        /* .card {
          display: flex;
          flex-direction: column;
          border-radius: 0.5rem;
          overflow: hidden;
          box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.15);
        } */

        /* card but with padding for formatting */
        .card {
          display: flex;
          flex-direction: column;
          border-radius: 0.5rem;
          overflow: hidden;
          box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.15);
          padding: 1rem;
        }

        .card__image {
          position: relative;
          padding-top: 56.25%;
          overflow: hidden;
          border-radius: 0.5rem;
        }

        .card__image img {
          position: absolute;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          object-fit: cover;
        }
      </style>
    </main>
  </div>
</BaseLayout>
