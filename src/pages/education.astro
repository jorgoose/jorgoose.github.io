---
import BaseLayout from "../layouts/BaseLayout.astro";
import Grid from "../components/Grid.astro";

import ContactCTA from "../components/ContactCTA.astro";
import Hero from "../components/Hero.astro";
---

<BaseLayout
  title="Projects | Logan Jorgensen"
  description="Learn about Logan Jorgensen's most recent projects"
>
  <div class="stack gap-20">
    <main class="wrapper stack gap-8 justify-content-center">
      <Hero
        title="Formal Education"
        tagline="See my most recent projects below to get an idea of my past experience."
        align="start"
      />
      <!-- List of previous employers with name of role and associated dates. They should each have a picture for a logo and centered on the page -->
      <div class="stack gap-8 justify-content-center">
        <div class="stack gap-4 justify-content-center">
          <div class="card gap-2 justify-content-center">
            <h3>Iowa State University</h3>
            <p>GPA: 3.8</p>
            <p>BS, Computer Science</p>
            <p>Minor: Data Science</p>
            <p>Ames, IA</p>
          </div>
          <!-- Horizontal Line to Seperate Job -->
          <hr class="line" />
          <div class="card gap-2 justify-content-center">
            <h3>Nihon University</h3>
            <p>Software Engineering Intern</p>
            <p>May 2023 - October 2022</p>
          </div>
          <hr class="line" />
          <div class="card gap-2 justify-content-center">
            <h3>Capital One</h3>
            <p>Software Engineering Intern</p>
            <p>June 2022 - August 2022</p>
          </div>
          <hr class="line" />
          <div class="card gap-2 justify-content-center">
            <h3>Iowa State University</h3>
            <p>IT Support Specialist</p>
            <p>November 2021 - June 2022</p>
          </div>
          <hr class="line" />
          <div class="card gap-2 justify-content-center">
            <h3>DoorDash</h3>
            <p>Delivery Driver</p>
            <p>February 2021 - November 2021</p>
          </div>
          <!-- Each Non-Software Project will be rendered in the grid -->
          <Hero title="Involvement" align="start" />
          <Grid variant="offset">
            <li>
              <div class="card">
                <div class="card__image">
                  <img
                    src="https://media.licdn.com/dms/image/D5622AQEFwwa82hidrw/feedshare-shrink_2048_1536/0/1682280226200?e=1692230400&v=beta&t=MSZPZFvn8oJgG2vH1fGixljseFAQqF3wDoGW6Th8ugQ"
                    alt="Other Project 1"
                  />
                </div>
                <div class="card__content">
                  <h3 class="card__title">Iowa State Investment Group</h3>
                  <!-- Similar to tag styling in the div below -->
