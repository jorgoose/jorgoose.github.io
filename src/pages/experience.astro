---
import BaseLayout from "../layouts/BaseLayout.astro";
import Grid from "../components/Grid.astro";
import Pill from "../components/Pill.astro";

import ContactCTA from "../components/ContactCTA.astro";
import Hero from "../components/Hero.astro";
---

<BaseLayout
  title="Experience | Logan Jorgensen"
  description="Learn about Logan Jorgensen's most recent projects"
>
  <div class="stack gap-20">
    <main class="wrapper stack gap-8 justify-content-center">
      <Hero
        title="Work Experience"
        tagline="See where I've worked and the path I've taken to get to where I am today."
        align="start"
      />
      <!-- List of previous employers with name of role and associated dates. They should each have a picture for a logo and centered on the page -->
      <div class="stack gap-8 justify-content-center">
        <div class="stack gap-4 justify-content-center">
          <div class="card gap-2 justify-content-center">
            <h3>Setwise Technology</h3>
            <p class="job-detail">Software Engineer Intern</p>
            <p class="job-detail">September 2023 - Present</p>
            <p class="job-detail">Ames, IA</p>
            <span class="card__tag">PHP · Laravel · Vue.js</span>
          </div>
          <!-- Horizontal Line to Seperate Job -->
          <hr class="line" />
          <div class="card gap-2 justify-content-center">
            <h3>Iowa State University</h3>
            <p class="job-detail">Peer Mentor (Computer Science)</p>
            <p class="job-detail">August 2023 - Present</p>
            <p class="job-detail">Ames, IA</p>
            <span class="card__tag">Git · Mentoring · Teaching</span>
          </div>
          <!-- Horizontal Line to Seperate Job -->
