          <!-- Horizontal Line to Seperate Job -->
          <hr class="line" />
          <div class="card gap-2 justify-content-center">
            <h3>Setwise Technology</h3>
            <p class="job-detail">Software Engineer Intern</p>
            <p class="job-detail">May 2023 - October 2022</p>
            <p class="job-detail">Ames, IA</p>
            <span class="card__tag">PHP · Laravel · Vue.js</span>
          </div>
